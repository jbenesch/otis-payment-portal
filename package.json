{
  "name": "otis-payment-portal",
  "version": "1.0.0",
  "description": "React dashboard for managing payments and invoices.",
  "main": "index.js",
  "repository": "git@github.com:jbenesch/otis-payment-portal.git",
  "author": "JB <jasonbenesch@gmail.com>",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@blueprintjs/core": "2.3.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "hedron": "0.7.1",
    "jest-enzyme": "6.0.1",
    "md5": "2.2.1",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-redux": "5.0.7",
    "react-scripts": "1.1.4",
    "react-transition-group": "2.3.1",
    "redux": "4.0.0",
    "redux-act": "1.7.4",
    "redux-form": "7.3.0",
    "redux-little-router": "15.1.1",
    "redux-mock-store": "1.5.1",
    "redux-saga": "0.16.0",
    "styled-components": "3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.6",
    "@storybook/addon-knobs": "3.4.6",
    "@storybook/addon-links": "3.4.6",
    "@storybook/addon-options": "3.4.6",
    "@storybook/addons": "3.4.6",
    "@storybook/react": "3.4.6",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "husky": "0.14.3",
    "lint-staged": "7.1.3",
    "prettier": "1.13.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  }
}
